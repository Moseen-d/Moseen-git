#include <iostream>
using namespace std;

/* Function to add two integers */
int add(int a, int b)
{
    return a + b;
}

/* Function to add two double values */
double add(double a, double b)
{
    return a + b;
}

int main()
{
    int x = 10, y = 20;
    double p = 12.5, q = 3.5;

    cout << "Addition of integers: " << add(x, y) << endl;
    cout << "Addition of double values: " << add(p, q) << endl;

    return 0;
}
